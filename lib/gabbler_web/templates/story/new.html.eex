<div class="create-story">
  <%= form_for :uploads, Routes.story_path(GabblerWeb.Endpoint, :upload, @hash), [multipart: true], fn f -> %>
    <div class="form-group">
      <label for="images">Choose JPG files to upload:</label>
      <input type="file" id="images" name="images[]" multiple /><br><br>
    </div>
    <div class="form-group">
      <input type="submit" />
    </div>
  <% end %>
</div>